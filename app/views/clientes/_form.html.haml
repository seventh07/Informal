= form_for @cliente do |f|
  - if @cliente.errors.any?
    #error_explanation
      %h2= "#{pluralize(@cliente.errors.count, "error")} prohibited this cliente from being saved:"
      %ul
        - @cliente.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :nome
    = f.text_field :nome
  .field
    = f.label :cpf
    = f.text_field :cpf
  .field
    = f.label :telefone
    = f.text_field :telefone
  .actions
    = f.submit 'Save'
